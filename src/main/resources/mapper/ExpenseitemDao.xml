<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ExpenseItemDao">
    <insert id="AddExpenseItem" keyProperty="reimId">
     insert into expenseitem (ContentId,ConsumeTime,Cost,Remark,Applicant) values
     (#{contentId},#{consumeTime},#{cost},#{remark},#{applicant})
    </insert>
</mapper>